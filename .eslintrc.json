{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base/legacy",
    "prettier"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "no-console": "off",
    "strict": "off",
    "no-plusplus": "off",
    "semi": "error"
  }
}
